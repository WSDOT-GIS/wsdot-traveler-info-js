{
  "name": "wsdot-traveler-info",
  "version": "1.1.0",
  "description": "Client for WSDOT Traveler Info API",
  "main": "TravelerInfoClient.js",
  "scripts": {
    "docs": "jsdoc2md TravelerInfoClient.js FerriesClient.js CommonUtils.js > api.md",
    "build": "tsc",
    "prepublish": "npm run build",
    "test": "jasmine && karma start",
    "lint": "tslint -c tslint.json -e \"typings/**/\" \"**/*.ts\"",
    "clean": "echo Removing compiled JavaScript... && del *.js *.map \"spec\\*Spec.js\" \"spec\\*.d.ts\" CommonUtils.d.ts FerriesClient.d.ts TravelerInfoClient.d.ts "
  },
  "files": [
    "CommonUtils.d.ts",
    "CommonUtils.js",
    "CommonUtils.js.map",
    "Ferries.d.ts",
    "FerriesClient.d.ts",
    "FerriesClient.js",
    "FerriesClient.js.map",
    "geoJsonUtils.js",
    "geoJsonUtils.js.map",
    "geoJsonUtils.d.ts",
    "index.d.ts",
    "TravelerInfo.d.ts",
    "TravelerInfoClient.d.ts",
    "TravelerInfoClient.js",
    "TravelerInfoClient.js.map"
  ],
  "keywords": [
    "WSDOT",
    "traveler",
    "traffic",
    "info",
    "information"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsdot-gis/wsdot-traveler-info-js.git"
  },
  "author": "WSDOT",
  "license": "Unlicense",
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/geojson": "0.0.31",
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "@types/whatwg-fetch": "0.0.32",
    "jasmine": "^2.5.2",
    "karma": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "requirejs": "^2.3.2",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "node-fetch": "^1.6.3"
  },
  "bugs": {
    "url": "https://github.com/wsdot-gis/wsdot-traveler-info-js/issues"
  },
  "homepage": "https://github.com/wsdot-gis/wsdot-traveler-info-js#readme"
}
