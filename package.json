  "devDependencies": {
    "jasmine": "^2.4.1"
  },
  "dependencies": {
    "node-fetch": "^1.5.2"
  },
  "bugs": {
    "url": "https://github.com/wsdot-gis/wsdot-traveler-info-js/issues"
  },
  "homepage": "https://github.com/wsdot-gis/wsdot-traveler-info-js#readme"
